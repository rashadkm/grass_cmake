PGM    = r.sim.water

#SIMDIR = $(SRC)/libes/simlib
#SIMLIB = $(LIBDIR)/simlib/simlib.a

SIMDIR = ../simlib
SIMLIB = ../simlib/simlib.a

EXTRA_CFLAGS = -I$(SIMDIR) -DPACKAGE=\"$(PGM)\"

LIST   =  main.o 
SRCS   =  main.c 

LIBES=$(SIMLIB) $(GISLIB) $(DATETIMELIB) $(BITMAPLIB) $(LINKMLIB) $(VECTLIB) $(DIG_ATTLIB)

DEPLIBS=$(SIMLIB) $(DEPGISLIB) $(DEPDATETIMELIB) $(DEPBITMAPLIB) $(DEPLINKMLIB) $(DEPVECTLIB) $(DEPDIG_ATTLIB)

$(BIN_CMD)/$(PGM): $(LIST) $(DEPLIBS)
	$(CC) $(LDFLAGS) -o $@ $(EXTRALDFLAGS) $(LIST) $(LIBES) $(MATHLIB) $(XDRLIB)

$(SIMLIB): #
$(DEPGISLIB): #
$(DEPDATETIMELIB): #
$(DEPBITMAPLIB): #
$(DEPLINKMLIB): #
$(DEPVECTLIB): #
